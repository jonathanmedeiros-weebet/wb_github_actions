{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "declaration": false,
    "emitDeclarationOnly": false,
    "types": ["vue-demi"],
    "moduleResolution": "Node",
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "stories"]
}